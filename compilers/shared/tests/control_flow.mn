# GUL Control Flow Test
# Tests if/elif/else, while

fn main():
    # If/else
    let x = 10
    if x < 5:
        print("small")
    else:
        print("large")
    
    # While loop
    var count = 0
    while count < 3:
        print(count)
        count = count + 1

# EXPECT: large
# EXPECT: 0
# EXPECT: 1
# EXPECT: 2
