fn bubble_sort(arr: @list):
    let n = len(arr)
    for i in range(n):
        for j in range(0, n-i-1):
            if arr[j] > arr[j+1]:
                let temp = arr[j]
                arr[j] = arr[j+1]
                arr[j+1] = temp
