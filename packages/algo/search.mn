fn binary_search(arr: @list, target: @int) -> @int:
    var low = 0
    var high = len(arr) - 1
    
    while low <= high:
        let mid = (low + high) / 2
        let val = arr[mid]
        if val == target: return mid
        if val < target: low = mid + 1
        else: high = mid - 1
        
    return -1
