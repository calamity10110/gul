
fn @bool is_alpha(ch: @str) -> @bool:
    let lower = "abcdefghijklmnopqrstuvwxyz"
    let res = ch in lower
    if not res:
        print("FAIL: " + ch)
    else:
        print("PASS: " + ch)
    return res

mn:
    let r1 = is_alpha("f")
    print("Result f: " + r1)
    
    let r2 = is_alpha("1")
    print("Result 1: " + r2)
