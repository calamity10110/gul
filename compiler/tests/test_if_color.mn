# Test using if/elif instead of match
enum Color:
    Red
    Blue
    Green

fn get_name(c: Color) -> @str:
    if c == Color.Red:
        return "red"
    elif c == Color.Blue:
        return "blue"
    else:
        return "other"

let r = Color.Red
let result = get_name(r)
print("Result: " + result)

mn:
    print("Done")
