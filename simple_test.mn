# GUL Simple Test - v3.2
# Basic syntax demonstration with @ prefix types

@imp std.io

struct Point:
    x: @int
    y: @int
    
    fn @str to_string(self):
        return "(" + str(self.x) + ", " + str(self.y) + ")"

mn:
    # Variables with @ prefix
    let message = @str("Hello from GUL v3.2!")
    let numbers = @list[1, 2, 3]
    let tuple_data = @tuple(1, 2, 3)
    let set_data = @set{1, 2, 3}
    
    # Dict with @ prefix
    let config = @dict{host: "localhost", port: 8080}
    
    # Struct usage
    let point = Point{x: 10, y: 20}
    
    # Output
    print(message)
    print("Sum:", 10 + 20)
    print("Numbers:", numbers)
    print("Point:", point.to_string())
    
    print("Done!")