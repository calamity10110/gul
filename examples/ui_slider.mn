# GUL UI Slider - v3.0

@imp std.ui

# v3.0: UI component literal syntax
let slider = ^&^[slider{min: 0, max: 100, value: 50}]

mn:
    # UI component rendering
    ui.print(slider)
    
    # Alternative: function call style (equivalent)
    ui.print(button[text: "Click me", color: "blue"])
    
    # Alternative: component literal style
    ^&^[label{text: "Hello World"}]
    
    print("Slider value:", slider.value)
